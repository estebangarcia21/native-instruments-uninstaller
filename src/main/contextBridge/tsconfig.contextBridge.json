{
  "compilerOptions": {
    "target": "ES2018",
    "module": "AMD",
    "lib": ["es5"],
    "strict": true,
    "pretty": true,
    "sourceMap": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "noImplicitAny": false
  }
}
